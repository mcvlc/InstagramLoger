        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <meta-data
            android:name="com.instagram.instagramapi.InstagramAppClientId"
            android:value="5b9b43e24bb94b70a6de3be1a0f041ff" />

        <meta-data
            android:name="com.instagram.instagramapi.InstagramAppRedirectURL"
            android:value="https://adlivetech.com/" />

        <activity android:name=".ui.activities.InstaActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application
        >

</manifest>